{
    "Logging": {
        "LogLevel": {
            "Default": "Information",
            "Microsoft.AspNetCore": "Information",
            "System.Net.Http": "Information"
        }
    },
    "AllowedHosts": "*",
    "MongoDb": {
        "Host": "localhost",
        "Port": 27017,
        "Username": "",
        "Password": ""
    },
    "MongoDbRepository": {
        "Database": "cash"
    },
    "MongoDbStorage": {
        "Database": "cash"
    },
    "RabbitMq": {
        "Host": "localhost",
        "Port": 5672,
        "Username": "",
        "Password": "",
        "VirtualHost": "/"
    },
    "RabbitMqIntegrationEventPublisher": {
        "ExchangeName": "cash.events",
        "ExchangeType": "topic"
    },
    "RabbitMqIntegrationEventConsumer": {
        "QueueName": "cash",
        "Bindings": [
            {"ExchangeName": "cash.events", "ExchangeType": "topic", "RoutingKey": "cash.*"},
            {"ExchangeName": "hand.events", "ExchangeType": "topic", "RoutingKey": "hand.cash.*"}
        ]
    },
    "RemoteHand": {
        "BaseUrl": "http://localhost"
    }
}
