{
    "Logging": {
        "LogLevel": {
            "Default": "Information",
            "Microsoft.AspNetCore": "Information",
            "System.Net.Http": "Information"
        }
    },
    "AllowedHosts": "*",
    "MongoDB": {
        "Host": "localhost",
        "Port": 27017,
        "Username": "",
        "Password": "",
        "Database": "cash"
    },
    "RabbitMqConnection": {
        "Host": "localhost",
        "Port": 5672,
        "Username": "",
        "Password": "",
        "VirtualHost": "/"
    },
    "RabbitMqIntegrationEventPublisher": {
        "ExchangeType": "topic",
        "ExchangeName": "cash.events"
    },
    "RabbitMqIntegrationEventConsumer": {
      "QueueName": "Hand",
      "Bindings": [
          {"ExchangeName": "cash.events", "RoutingKey": "cash.*" }
      ]
    },
    "RemoteHand": {
        "BaseUrl": "http://localhost"
    }
}
